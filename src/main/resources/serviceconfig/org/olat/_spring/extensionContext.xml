		<property name="i18nDescriptionKey" value="main.menu.title.alt" />
		<property name="extensionPoints">
			<list>
				<value>org.olat.course.statistic.StatisticMainController</value>
			</list>
		</property>
	</bean>

	<bean class="org.olat.course.statistic.StatisticActionExtension"
		init-method="initExtensionPoints" id="dailyextension">
		<property name="actionController">
			<bean class="org.olat.course.statistic.StatisticAutoCreator"
				scope="prototype">
				<property name="className"
					value="org.olat.course.statistic.daily.DailyStatisticDisplayController" />
				<property name="statisticManager">
					<bean class="org.olat.course.statistic.daily.DailyStatisticManager" />
				</property>
			</bean>
		</property>
		<property name="i18nActionKey" value="menu.createstatfile" />
		<property name="i18nDescriptionKey" value="menu.createstatfile.alt" />
		<!-- the translationPackage is used to make sure the i18n properties are 
			read from the right package -->
		<property name="translationPackage" value="org.olat.course.statistic.daily" />
		<property name="extensionPoints">
			<list>
				<value>org.olat.course.statistic.StatisticMainController</value>
			</list>
		</property>
		<property name="order" value="4" />
	</bean>

